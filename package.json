{
  "name": "express-typeorm-postgres-boilerplate",
  "version": "1.0.0",
  "description": "express-typeorm-postgres-boilerplate",
  "scripts": {
    "dev": "NODE_ENV=development nodemon --exec ts-node -r tsconfig-paths/register --files src/index.ts",
    "clean": "rm -rf build",
    "build": "npm -s run clean && tsc --build tsconfig.json",
    "start": "NODE_ENV=production node build/index.js"
  },
  "dependencies": {
    "express": "^4.15.4",
    "pg": "^8.4.0",
    "reflect-metadata": "^0.1.10",
    "ts-node": "^9.1.1",
    "typeorm": "^0.2.32",
    "typescript": "^4.2.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/node": "^8.0.29",
    "nodemon": "^2.0.7",
    "tsconfig-paths": "^3.9.0"
  }
}
